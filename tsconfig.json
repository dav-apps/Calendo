{
  "compileOnSave": false,
  "compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "ES2020",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "ES2020",
		"typeRoots": ["node_modules/@types"],
		"lib": ["ES2020", "dom"],
		"skipLibCheck": true,
		"downlevelIteration": true
  }
}
